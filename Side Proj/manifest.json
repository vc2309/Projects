{
    "name": "Young",
    "version": "1.0",
    "manifest_version": 2,
    "description": "First extension",   
    "browser_action": {
        "default_icon": "myicon.png",
        "default_popup": "index.html"
    },
    "background": {
    "scripts": ["check.js","background.js"],
    "persistent": false
  },   
    "content_scripts": [{
        "matches": [ "http://*/*", "https://*/*" ],  
        "js": [ "jquery.min.js", "main.js"]

}],
"permissions": ["activeTab","storage","notifications","webNavigation"]
}